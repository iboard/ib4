<% form_for @note do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :parent_id %><br />
    <%= f.text_field :parent_id %>
  </p>
  <p>
    <%= f.label :user_id %><br />
    <%= f.text_field :user_id %>
  </p>
  <p>
    <%= f.label :noteable_type %><br />
    <%= f.text_field :noteable_type %>
  </p>
  <p>
    <%= f.label :noteable_id %><br />
    <%= f.text_field :noteable_id %>
  </p>
  <p>
    <%= f.label :message %><br />
    <%= f.text_area :message %>
  </p>
  <p>
    <%= f.label :message_value %><br />
    <%= f.text_field :message_value %>
  </p>
  <p>
    <%= f.label :message_type %><br />
    <%= f.select :message_type, Note::MESSAGE_TYPES.map { |t|[ t.to_s.humanize, t ]} %>
  </p>
  <p><%= f.submit "Submit" %></p>
<% end %>
