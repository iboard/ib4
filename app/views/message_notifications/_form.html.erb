<% form_for @message_notification do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :message_id %><br />
    <%= f.text_field :message_id %>
  </p>
  <p>
    <%= f.label :user_id %><br />
    <%= f.text_field :user_id %>
  </p>
  <p>
    <%= f.label :read_at %><br />
    <%= f.datetime_select :read_at %>
  </p>
  <p><%= f.submit "Submit" %></p>
<% end %>
